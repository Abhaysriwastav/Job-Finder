  <script>
      $(document).ready(function () {
          document.querySelectorAll('.job').forEach(jobContainer => adjustJobPanel(jobContainer))
          checkVisaAndEligibilityTags();
      });

      function adjustJobPanel(jobContainer) {
          shortenEmployerName(jobContainer.querySelector('.employer'));
          collapseJobClassifications(jobContainer.querySelector('.job-classifications'));
      }

      function shortenEmployerName(employerContainer) {
          const sponsorElement = employerContainer.querySelector('.sponsor');
          const sponsorWidth = sponsorElement ? sponsorElement.offsetWidth : 0;
          const maxEmployerName = Math.floor((employerContainer.offsetWidth * 2 - sponsorWidth) / 9); // Divided by 9 since the average character width is nearly 8px
          const employerNameElement = employerContainer.querySelector('.employer-name');
          const originalEmployerName = employerNameElement.innerHTML;

          if (originalEmployerName.length > maxEmployerName) {
              const truncatedEmployerName = originalEmployerName.substring(0, maxEmployerName - 1);
              const lastSpaceIndex = truncatedEmployerName.lastIndexOf(' ');
              employerNameElement.innerHTML = truncatedEmployerName.substring(0, lastSpaceIndex);
          }
      }

      function collapseJobClassifications(classificationContainer) {
          let containerWidth = classificationContainer.offsetWidth;
          let totalItemWidth = 0;
          let hiddenItems = 0;

          classificationContainer.querySelectorAll('.job-classification').forEach(classification => {
              totalItemWidth += classification.offsetWidth;

              if (totalItemWidth + 70 > containerWidth) {
                  hiddenItems++;
                  classification.classList.remove('d-inline-flex');
                  classification.style.display = 'none';
              }
          })

          if (hiddenItems > 0) {
              classificationContainer.insertAdjacentHTML('beforeend', `<div class="rounded-5 px-2 py-1 gap-1 text-nowrap tag sub-font" style="background-color:#F0EBEB;color:gray">+${hiddenItems}</div>`)
          }
      }

      // Collapse visa and eligibility tags
      function checkVisaAndEligibilityTags() {
          const tagContainers = document.querySelectorAll('.tags');
          tagContainers.forEach(tagContainer => checkVisaAndEligibilityTagsInEachJob(tagContainer))
      }

      function checkVisaAndEligibilityTagsInEachJob(tagContainer) {
          let containerWidth = tagContainer.offsetWidth;
          let tagsWidth = 0;
          let hiddenItems = 0;

          tagContainer.querySelectorAll('.tag').forEach(tag => {

              tagsWidth += tag.offsetWidth;

              if (tagsWidth + 70 > containerWidth) {
                  hiddenItems++;
                  tag.style.display = 'none';
              }
          })

          if (hiddenItems > 0) {
              tagContainer.insertAdjacentHTML('beforeend', `<div class="rounded-5 px-2 py-1 gap-1 text-nowrap tag sub-font" style="background-color:#F0EBEB;color:gray">+${hiddenItems}</div>`)
          }
      }
  </script>

