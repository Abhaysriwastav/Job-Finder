<body class="d-flex flex-column min-vh-100">
<script>

    $(document).ready(() => {
        setNavBarActive();
    });

    function toggleNav() {
        document.getElementById('navbarCollapse').classList.toggle('show');
        document.getElementById('navbarTogglerIcon').classList.toggle('show');
        document.body.classList.toggle('lock-scroll');
    }

    function setNavBarActive() {
        const currentUrl = window.location.href;
        const navElement = document.getElementById('navbarCollapse').querySelector('.navbar-nav').children;

        Array.from(navElement).forEach(navItem => {

            if (currentUrl?.includes(navItem.href)) {
                navItem.classList.add('active');
            }

        })
    }
</script>

