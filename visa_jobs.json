<script>
    $(document).ready(function () {
        hideChipsIfNeeded('classificationTags', 'py-1 back-gray');
        setChipsForDropdown('country-dropdown', 'Any Country', null);
        setChipsForDropdown('industry-classification-dropdown', 'Any Classification', null);

        closeDropdownsOnCLickOutside();
        deleteFormEmptyFields();
        removeInputGroupClassOnSmallScreens();
        removeHiddenExtraOptionsFromDom();
        displayExtraOptionSelectedCounts();
    });

    function hideChipsIfNeeded(id, hiddenItemClass) {

        if (!id) {
            return;
        }

        let container = document.getElementById(id);

        if (!container) {
            return;
        }

        let containerWidth = container.offsetWidth;
        let tagsWidth = 0;
        let hiddenItems = 0;

        container.querySelectorAll('.tag').forEach(tag => {
            tagsWidth += tag.offsetWidth;

            if (tagsWidth + 75 > containerWidth) {
                hiddenItems++;
                tag.style.setProperty('display', 'none', 'important');
            }
        });

        if (hiddenItems > 0) {
            container.insertAdjacentHTML('beforeend', `<div class="rounded-5 px-1 gap-1 text-nowrap d-inline-flex tag hiddenItem sub-font ${hiddenItemClass}">+${hiddenItems}</div>`)
        }
    }

    function setChipsForDropdown(id, defaultText, event) {
        if (event) {
            event.preventDefault();
        }

        const container = document.getElementById(id);
        const dropdownToggle = container.querySelector('.dropdown-toggle');
        const dropdownMenuItemElements = container.querySelector('.dropdown-menu').children;

        dropdownToggle.innerHTML = '';

        let checkedItems = 0;

        Array.from(dropdownMenuItemElements).forEach(child => {
            const labelName = child.querySelector('label').innerText;
            const inputEl = child.querySelector('input');

            if (inputEl.checked) {
                checkedItems++;
                const chipHtml = `<span class="rounded-5 px-1 mx-1 d-inline-flex align-items-center tag back-blue sub-font">
                <span class="mx-1 text-nowrap">${labelName}</span>
                <span onclick="deleteChip(this, event)">
                    <svg width="17" height="16" viewBox="0 0 17 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M13.4318 12.2349C13.4686 12.2693 13.4982 12.3107 13.5187 12.3567C13.5392 12.4027 13.5502 12.4523 13.5511 12.5027C13.552 12.553 13.5427 12.603 13.5239 12.6497C13.505 12.6964 13.4769 12.7388 13.4413 12.7744C13.4057 12.8101 13.3633 12.8381 13.3166 12.857C13.2699 12.8758 13.2199 12.8851 13.1695 12.8842C13.1192 12.8833 13.0695 12.8723 13.0235 12.8518C12.9775 12.8313 12.9361 12.8018 12.9018 12.7649L8.66679 8.53055L4.43179 12.7649C4.36071 12.8312 4.26668 12.8672 4.16953 12.8655C4.07238 12.8638 3.97969 12.8244 3.91098 12.7557C3.84228 12.687 3.80292 12.5943 3.80121 12.4972C3.79949 12.4 3.83555 12.306 3.90179 12.2349L8.13617 7.99992L3.90179 3.76492C3.83555 3.69384 3.79949 3.59981 3.80121 3.50266C3.80292 3.40551 3.84228 3.31282 3.91098 3.24411C3.97969 3.17541 4.07238 3.13605 4.16953 3.13434C4.26668 3.13262 4.36071 3.16868 4.43179 3.23492L8.66679 7.4693L12.9018 3.23492C12.9729 3.16868 13.0669 3.13262 13.1641 3.13434C13.2612 3.13605 13.3539 3.17541 13.4226 3.24411C13.4913 3.31282 13.5307 3.40551 13.5324 3.50266C13.5341 3.59981 13.498 3.69384 13.4318 3.76492L9.19742 7.99992L13.4318 12.2349Z" fill="#005E87"/>
                    </svg>
                </span>
                </span>`;

                $(dropdownToggle).append(chipHtml);

                $(child).addClass('back-blue');
            } else {
                $(child).removeClass('back-blue');
            }
        });

        if (checkedItems === 0) {
            dropdownToggle.innerHTML = `<span class="m-2 sub-font">${defaultText}</span>`;
        }

        hideChipsIfNeeded(dropdownToggle.id, 'mx-1 back-blue');
    }

    function toggleDropdownItemBackground(toggledElement) {
        const dropdownMenu = toggledElement.closest('.dropdown-menu');
        toggleDropdownItemSiblings(toggledElement, dropdownMenu);
        highlightSelectedDropdownItems(dropdownMenu);
    }

    function highlightDropdownMenu(toggle) {
        toggle.querySelector('svg').classList.toggle('rotated')
        const dropdownMenu = toggle.closest('.dropdown').querySelector('.dropdown-menu');
        highlightSelectedDropdownItems(dropdownMenu);
        const dropdownItems = dropdownMenu.querySelectorAll('input');
        setTheIndeterminateStateOnTheAllOptionsCheckbox(dropdownItems);
    }

    function toggleDropdownItemSiblings(toggledElement, dropdownMenu) {
        const dropdownItems = dropdownMenu.querySelectorAll('input');

        if ($(toggledElement).hasClass('all-options')) {
            Array.from(dropdownItems)
                .filter(item => item.classList.contains('single-option'))
                .forEach(item => item.checked = toggledElement.checked);
        } else {
            setTheIndeterminateStateOnTheAllOptionsCheckbox(dropdownItems);
        }

        const numberOfCheckedItems = Array.from(dropdownItems)
            .filter(item => $(item).hasClass('single-option') && item.checked)
            .length;
        const dropdownElement = dropdownMenu.closest('.dropdown');
        insertNumberOfCheckedItemsBadge(dropdownElement, numberOfCheckedItems);
    }

    function setTheIndeterminateStateOnTheAllOptionsCheckbox(dropdownItems) {
        const numberOfCheckedItems = Array.from(dropdownItems)
            .filter(item => $(item).hasClass('single-option') && item.checked)
            .length;
        const numberOfUncheckedItems = Array.from(dropdownItems)
            .filter(item => $(item).hasClass('single-option') && !item.checked)
            .length;

        Array.from(dropdownItems).forEach(item => {
            if ($(item).hasClass('all-options')) {
                if (numberOfCheckedItems && numberOfUncheckedItems) {
                    item.indeterminate = true;
                    item.checked = false;
                    return;
                }

                item.indeterminate = false;
                if (numberOfCheckedItems) {
                    item.checked = true;
                } else if (numberOfUncheckedItems) {
                    item.checked = false;
                }
            }
        })
    }

    function highlightSelectedDropdownItems(dropdownMenu) {
        Array.from(dropdownMenu.children).forEach(menuChild => {
            const inputElement = menuChild.querySelector('input');
            if (inputElement.checked) {
                $(menuChild).addClass('back-blue');
            } else {
                $(menuChild).removeClass('back-blue');
            }
        });
    }

    function insertNumberOfCheckedItemsBadge(dropdownElement, numberOfCheckedItems) {
        const existingBadge = dropdownElement.querySelector('.checked-items-badge');
        if (existingBadge) {
            existingBadge.remove();
        }

        if (numberOfCheckedItems) {
            const caretElement = dropdownElement.querySelector('svg');
            caretElement.insertAdjacentHTML('beforebegin', `<div class="rounded-5 ms-2 px-1 sub-font background-baby-blue checked-items-badge">${numberOfCheckedItems}</div>`)
        }
    }

    function toggleDropdown(dropdownId, e) {
        e.stopPropagation();

        $(document.getElementById(dropdownId)).dropdown('toggle');
        closeDropdowns(dropdownId);
    }

    function deleteChip(deleteItem, event) {
        event.stopPropagation();
        event.preventDefault();
        event.stopImmediatePropagation();

        const text = $(deleteItem).parent().find('.text-nowrap').text();
        const dropdownMenu = $(deleteItem).parent().parent().parent().find('.dropdown-menu');
        const items = Array.from(dropdownMenu.children());

        items.forEach(item => {
            const label = $(item).find('label');

            if (label.text() === text) {
                label.click();
            }

        })
    }

    function closeDropdownsOnCLickOutside() {
        $(document).click(() => {
            closeDropdowns();
        });
    }

    function closeDropdowns(exceptId) {
        [ '.dropdown-toggle' ].forEach(item => {
            $(item).each((index, element) => {

                if (element.id !== exceptId) {
                    $(element).dropdown("hide");
                }
            });
        })
    }

    function deleteFormEmptyFields() {
        const forms = document.querySelectorAll('form');
        forms.forEach(form => {
            form.addEventListener('formdata', function (event) {
                const formData = event.formData;
                for (const [ name, value ] of Array.from(formData.entries())) {
                    if (!value) {
                        formData.delete(name);
                    }
                }
            });
        })
    }

    function removeInputGroupClassOnSmallScreens() {
        const isSmallScreen = screen.width < 992;
        if (isSmallScreen) {
            const emailInputGroup = document.getElementById('email-input-group');
            if (emailInputGroup) {
                emailInputGroup.classList.remove('input-group');
            }
        }
    }

    function removeHiddenExtraOptionsFromDom() {
        // We need to remove the block even it's already hidden to avoid sending duplicate input values
        const hiddenId = `extra-search-options-dropdown-${window.innerWidth < 992 ? 'large' : 'small'}`;
        document.getElementById(hiddenId).remove();
    }

    function displayExtraOptionSelectedCounts() {
        const displayElements = document.querySelectorAll('.extra-search-options-dropdown');
        displayElements.forEach(element => {
            const dropdownElements = element.querySelectorAll('.dropdown');
            dropdownElements.forEach(dropdownElement => {
                if (dropdownElement.classList.contains('visa')) {
                    insertNumberOfCheckedItemsBadge(dropdownElement, 0);
                } else if (dropdownElement.classList.contains('education')) {
                    insertNumberOfCheckedItemsBadge(dropdownElement, 0);
                } else if (dropdownElement.classList.contains('experience')) {
                    insertNumberOfCheckedItemsBadge(dropdownElement, 0);
                }
            })
        });
    }

    function setEmailAlert(form) {
        if (form.reportValidity()) {
            const formData = new FormData(form);
            formData.set('query', window.location.search)

            fetch(new URL(form.action), {
                method: 'POST',
                body: formData
            }).then(function (response) {
                return response.text();
            }).then(function (html) {
                const feedbackElement = $('#email-registration-feedback');
                feedbackElement.html(html);
                feedbackElement.show();
                $('#email-registration-form').hide();
            });
        }
    }

    function toggleExtraSearchOptions(buttonId) {
        toggleOptionButtonText(buttonId);
        toggleExtraOptionDisplay();
    }

    const MORE_OPTIONS = 'MORE OPTIONS';
    const HIDE_OPTIONS = 'HIDE OPTIONS';

    function toggleOptionButtonText(buttonId) {
        const buttonElement = document.getElementById(buttonId);
        const currentText = buttonElement.innerText;
        buttonElement.innerText = currentText === MORE_OPTIONS ? HIDE_OPTIONS : MORE_OPTIONS;

        const extraSearchOptionsElement = document.getElementById("show-extra-search-options");
        extraSearchOptionsElement.value = extraSearchOptionsElement.value === 'false' ? 'true' : "false";
    }

    function toggleExtraOptionDisplay() {
        const displayElements = document.querySelectorAll('.extra-search-options-dropdown');
        displayElements.forEach(element => element.classList.toggle('show'));
    }
</script>
